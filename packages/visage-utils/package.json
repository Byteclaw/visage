{
  "name": "@byteclaw/visage-utils",
  "version": "0.3.0",
  "description": "Collection of utilities used by Visage Design System.",
  "bugs": {
    "url": "https://github.com/Byteclaw/visage/issues"
  },
  "homepage": "https://github.com/Byteclaw/visage/packages/visage-utils/README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/Byteclaw/visage.git"
  },
  "keywords": [
    "visage",
    "design system",
    "react",
    "css in js",
    "react-native",
    "utilities"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/byteclaw.visage.utils.esm.js",
  "unpkg": "dist/byteclaw.visage.utils.umd.production.min.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist",
    "src",
    "!src/**/__tests__/**/*"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "bundle": "tsdx build --format cjs,esm  --name byteclaw.visage.utils --entry src/index.ts",
    "build": "tsdx build --format cjs,esm,umd --name byteclaw.visage.utils --entry src/index.ts",
    "prepublishOnly": "tsdx build --format cjs,esm,umd  --name byteclaw.visage.utils --entry src/index.ts",
    "test:types": "tsc -p . --noEmit"
  }
}
